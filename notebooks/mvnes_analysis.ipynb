   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "from pathlib import Path\n",
    "\n",
    "# Set plot style\n",
    "sns.set_style('whitegrid')\n",
    "plt.rcParams['figure.figsize'] = (12, 6)\n",
    "\n",
    "# Load the data\n",
    "data_dir = Path('../data')\n",
    "results_file = data_dir / 'mvnes_gng_results.csv'\n",
    "\n",
    "if not results_file.exists():\n",
    "    print(f'Error: Results file not found at {results_file}')\n",
    "    raise FileNotFoundError(f'Results file not found at {results_file}')\n",
    "\n",
    "df = pd.read_csv(results_file)\n",
    "print(f'Loaded {len(df)} trials from {results_file}')\n",
    "\n",
    "# Display basic statistics\n",
    "print("\nBasic Statistics:")\n",
    "print(df.describe())\n",
    "\n",
    "# Display outcome distribution\n",
    "print("\nOutcome Distribution:")\n",
    "print(df['outcome'].value_counts(normalize=True))"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Reaction Time Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Filter out timeout trials\n",
    "df_non_timeout = df[~df['timeout']]\n",
    "\n",
    "# Create separate dataframes for Go and NoGo trials\n",
    "go_trials = df_non_timeout[df_non_timeout['stimulus'] == 'go']\n",
    "nogo_trials = df_non_timeout[df_non_timeout['stimulus'] == 'nogo']\n",
    "\n",
    "# Plot RT distributions\n",
    "plt.figure(figsize=(12, 6))\n",
    "sns.kdeplot(go_trials['rt'], label='Go Trials', shade=True)\n",
    "sns.kdeplot(nogo_trials['rt'], label='NoGo Trials', shade=True)\n",
    "plt.title('Reaction Time Distributions (Go vs NoGo)')\n",
    "plt.xlabel('Reaction Time (s)')\n",
    "plt.ylabel('Density')\n",
    "plt.legend()\n",
    "plt.show()\n",
    "\n",
    "# Calculate and print mean RTs\n",
    "print("\nMean Reaction Times:")\n",
    "print(f'Go Trials: {go_trials['rt'].mean():.3f} s')\n",
    "print(f'NoGo Trials: {nogo_trials['rt'].mean():.3f} s')"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# False Alarm Rate vs w_n Analysis"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate false alarm rate for different w_n values\n",
    "# Assuming we have multiple simulations with different w_n values\n",
    "def calculate_fa_rate(df):\n",
    "    nogo_trials = df[df['stimulus'] == 'nogo']\n",
    "    fa_rate = (nogo_trials['outcome'] == 'False Alarm').mean()\n",
    "    return fa_rate\n",
    "\n",
    "# Example: If we have different w_n values in the data\n",
    "if 'w_n' in df.columns:\n",
    "    # Group by w_n and calculate FA rate\n",
    "    fa_rates = df.groupby('w_n').apply(calculate_fa_rate)\n",
    "    \n",
    "    plt.figure(figsize=(12, 6))\n",
    "    plt.plot(fa_rates.index, fa_rates.values, marker='o')\n",
    "    plt.title('False Alarm Rate vs w_n')\n",
    "    plt.xlabel('w_n (Norm Weight)')\n",
    "    plt.ylabel('False Alarm Rate')\n",
    "    plt.grid(True)\n",
    "    plt.show()\n",
    "else:\n",
    "    print("Warning: w_n column not found in data. Cannot plot FA rate vs w_n.")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# Decision Trace Visualization\n",
    "(If evidence traces were saved)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Function to visualize decision traces\n",
    "def plot_decision_trace(trial_data, trial_idx):\n",
    "    \"\"\"Plot evidence accumulation trace for a single trial.\"\"\"\n",
    "    plt.figure(figsize=(12, 4))\n",
    "    \n",
    "    # Plot evidence trace\n",
    "    plt.plot(trial_data['trace'], label='Evidence')\n",
    "    \n",
    "    # Add threshold line\n",
    "    threshold = trial_data.get('threshold_a', None)\n",
    "    if threshold is not None:\n",
    "        plt.axhline(y=threshold, color='r', linestyle='--', label='Threshold')\n",
    "    \n",
    "    plt.title(f'Trial {trial_idx} - {trial_data['outcome']}')\n",
    "    plt.xlabel('Time Steps')\n",
    "    plt.ylabel('Evidence')\n",
    "    plt.legend()\n",
    "    plt.grid(True)\n",
    "\n",
    "# Example: Plot traces for different trial types\n",
    "if 'trace' in df.columns:\n",
    "    # Convert trace strings back to lists\n",
    "    \"\"\"Note: This assumes traces were saved as strings in the CSV.\n",
    "    If they were saved as lists, this conversion won't be needed.\"\"\"\n",
    "    df['trace'] = df['trace'].apply(eval)\n",
    "    \n",
    "    # Find example trials\n",
    "    hit_trial = df[df['outcome'] == 'Hit'].iloc[0]\n",
    "    fa_trial = df[df['outcome'] == 'False Alarm'].iloc[0]\n",
    "    cr_trial = df[df['outcome'] == 'Correct Rejection'].iloc[0]\n",
    "    \n",
    "    # Plot example traces\n",
    "    plt.figure(figsize=(15, 12))\n",
    "    \n",
    "    plt.subplot(3, 1, 1)\n",
    "    plot_decision_trace(hit_trial, hit_trial['trial'])\n",
    "    \n",
    "    plt.subplot(3, 1, 2)\n",
    "    plot_decision_trace(fa_trial, fa_trial['trial'])\n",
    "    \n",
    "    plt.subplot(3, 1, 3)\n",
    "    plot_decision_trace(cr_trial, cr_trial['trial'])\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "    \n",
    "    # Calculate and plot average traces by outcome\n",
    "    def get_average_trace(df_subset):\n",
    "        traces = df_subset['trace'].tolist()\n",
    "        max_len = max(len(t) for t in traces)\n",
    "        avg_trace = np.zeros(max_len)\n",
    "        counts = np.zeros(max_len)\n",
    "        \n",
    "        for trace in traces:\n",
    "            avg_trace[:len(trace)] += trace\n",
    "            counts[:len(trace)] += 1\n",
    "        \n",
    "        avg_trace = avg_trace / np.maximum(counts, 1)\n",
    "        return avg_trace\n",
    "    \n",
    "    plt.figure(figsize=(12, 8))\n",
    "    \n",
    "    # Plot average traces\n",
    "    outcomes = ['Hit', 'False Alarm', 'Correct Rejection']\n",
    "    for outcome in outcomes:\n",
    "        subset = df[df['outcome'] == outcome]\n",
    "        if len(subset) > 0:\n",
    "            avg_trace = get_average_trace(subset)\n",
    "            plt.plot(avg_trace, label=f'Avg {outcome}')\n",
    "    \n",
    "    plt.title('Average Evidence Accumulation by Outcome')\n",
    "    plt.xlabel('Time Steps')\n",
    "    plt.ylabel('Evidence')\n",
    "    plt.legend()\n",
    "    plt.grid(True)\n",
    "    plt.show()\n",
    "else:\n",
    "    print("Warning: Trace data not found in CSV. Cannot plot decision traces.")"
   ]
  }
